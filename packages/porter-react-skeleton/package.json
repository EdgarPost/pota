{
  "name": "@mediamonks/porter-react-skeleton",
  "version": "0.0.2-3",
  "author": "Paul <p.simkevi@gmail.com>",
  "bugs": {
    "url": "https://github.com/mediamonks/porter/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mediamonks/porter.git",
    "directory": "packages/porter-react-skeleton"
  },
  "scripts": {
    "check-types": "tsc --project tsconfig.json --noEmit --noUnusedLocals",
    "fix": "npm-run-all fix:*",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "prettier \"**/*.{js,jsx,ts,tsx,scss}\" --write --loglevel warn",
    "fix:stylelint": "npm run lint:stylelint -- --fix",
    "lint": "npm-run-all lint:*",
    "lint:eslint": "eslint . --ext .ts,.tsx --cache --cache-location node_modules/.cache/.eslintcache",
    "lint:stylelint": "stylelint --syntax css-in-js \"src/**/*.{ts,tsx}\" --cache --cache-location node_modules/.cache/.stylelintcache",
    "upload-build": "cross-env rsync -avPx build/* ${npm_package_name}@${npm_config_host}:~/public/$FOLDER --delete-after",
    "deploy": "ts-node-transpile-only --script-mode build-tools/scripts/deploy.ts"
  },
  "type": "module",
  "files": [
    "porter.ts",
    "tsconfig.json",
    "tsconfig.tools.json",
    "public",
    "src",
    "build-tools",
    "generator-template",
    "gitignore",
    ".editorconfig",
    ".stylelintrc",
    ".eslintrc.js",
    ".huskyrc",
    ".nvmrc",
    ".prettierrc",
    ".senggenerator",
    "bitbucket-pipelines.yml",
    "lint-staged.config.js"
  ],
  "dependencies": {
    "@mediamonks/eslint-config-react": "^1.0.2",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.6",
    "@types/styled-components": "^5.1.4",
    "cross-env": "^7.0.3",
    "eslint": "^7.19.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "framer-motion": "^2.9.4",
    "husky": "^4.3.0",
    "isntnt": "^1.4.1",
    "lint-staged": "^10.5.1",
    "mobx": "^6.0.4",
    "mobx-react-lite": "^3.1.6",
    "npm-run-all": "^4.1.5",
    "polished": "^4.0.3",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "seng-generator": "^0.9.0",
    "sort-package-json": "^1.44.0",
    "styled-components": "^5.2.1",
    "stylelint": "^13.10.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "web-vitals": "^1.1.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
