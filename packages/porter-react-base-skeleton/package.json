{
  "name": "@mediamonks/porter-react-base-skeleton",
  "version": "0.0.1",
  "author": "Paul <p.simkevi@gmail.com>",
  "bugs": {
    "url": "https://github.com/mediamonks/porter/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mediamonks/porter.git",
    "directory": "packages/porter-react-base-skeleton"
  },
  "scripts": {
    "check-types": "tsc --project tsconfig.json --noEmit --noUnusedLocals",
    "fix": "npm-run-all fix:*",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "prettier \"**/*.{js,jsx,ts,tsx,scss}\" --write --loglevel warn",
    "fix:stylelint": "npm run lint:stylelint -- --fix",
    "lint": "npm-run-all lint:*",
    "lint:eslint": "eslint . --ext .ts,.tsx --cache --cache-location node_modules/.cache/.eslintcache",
    "lint:stylelint": "stylelint \"{components,website}/**/*.{ts,tsx}\" --cache --cache-location \"node_modules/.cache/.stylelintcache/\""
  },
  "type": "module",
  "files": [
    "porter.ts",
    "tsconfig.json",
    "tsconfig.tools.json",
    "public",
    "src",
    "gitignore",
    ".editorconfig",
    ".eslintrc.js",
    ".huskyrc",
    ".nvmrc",
    ".prettierrc",
    ".senggenerator",
    "bitbucket-pipelines.yml",
    "lint-staged.config.js"
  ],
  "dependencies": {
    "@mediamonks/eslint-config": "^1.2.3",
    "@mediamonks/porter-dev-utils": "workspace:packages/porter-dev-utils",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.2.2",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "@vue/cli-plugin-typescript": "^4.5.11",
    "@vue/cli-service": "^4.5.11",
    "@vue/eslint-config-airbnb": "^5.3.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "babel-eslint": "^10.1.0",
    "confusing-browser-globals": "^1.0.10",
    "cross-env": "^7.0.3",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-html": "^6.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unicorn": "27.0.0",
    "eslint-plugin-vue": "^7.5.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "seng-generator": "^0.9.0",
    "sort-package-json": "^1.44.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "web-vitals": "^1.1.0"
  }
}
