{
  "name": "@mediamonks/porter-react-base-skeleton",
  "version": "0.0.2-3",
  "author": "Paul <p.simkevi@gmail.com>",
  "bugs": {
    "url": "https://github.com/mediamonks/porter/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mediamonks/porter.git",
    "directory": "packages/porter-react-base-skeleton"
  },
  "scripts": {
    "check-types": "tsc --project tsconfig.json --noEmit --noUnusedLocals",
    "fix": "npm-run-all fix:*",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "prettier \"**/*.{js,jsx,ts,tsx,scss}\" --write --loglevel warn",
    "lint": "npm-run-all lint:*",
    "lint:eslint": "eslint . --ext .ts,.tsx --cache --cache-location node_modules/.cache/.eslintcache"
  },
  "type": "module",
  "files": [
    "porter.ts",
    "tsconfig.json",
    "tsconfig.tools.json",
    "public",
    "src",
    "gitignore",
    ".editorconfig",
    ".eslintrc.js",
    ".huskyrc",
    ".nvmrc",
    ".prettierrc",
    ".senggenerator",
    "bitbucket-pipelines.yml",
    "lint-staged.config.js"
  ],
  "dependencies": {
    "@mediamonks/eslint-config-react": "^1.0.2",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.19.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "seng-generator": "^0.9.0",
    "sort-package-json": "^1.44.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "web-vitals": "^1.1.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
