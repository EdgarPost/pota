{
  "name": "@pota/webpack-skeleton",
  "version": "1.0.0-rc.9",
  "author": "Paul <p.simkevi@gmail.com>",
  "type": "module",
  "bugs": {
    "url": "https://github.com/mediamonks/pota/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mediamonks/pota.git",
    "directory": "skeletons/webpack"
  },
  "files": [
    "public",
    "src",
    "build-tools",
    "pota_commands",
    "gitignore",
    "tsconfig.json",
    ".eslintrc.cjs",
    ".editorconfig",
    ".huskyrc",
    ".nvmrc",
    ".prettierrc",
    "bitbucket-pipelines.yml",
    "lint-staged.config.js"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=development webpack -c webpack/webpack.config.js",
    "dev": "cross-env NODE_ENV=development webpack serve -c webpack/webpack.config.js",
    "check-types": "tsc --project tsconfig.json --noEmit --noUnusedLocals",
    "fix": "npm-run-all fix:*",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "prettier \"**/*.{js,jsx,ts,tsx,scss}\" --write --loglevel warn",
    "lint": "npm-run-all lint:*",
    "lint:eslint": "eslint . --ext .ts,.tsx --cache --cache-location node_modules/.cache/.eslintcache"
  },
  "dependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@mediamonks/eslint-config": "^1.2.3",
    "@squoosh/lib": "^0.4.0",
    "@types/friendly-errors-webpack-plugin": "^0.1.4",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "autoprefixer": "^10.3.6",
    "babel-loader": "^8.2.2",
    "confusing-browser-globals": "^1.0.10",
    "copy-webpack-plugin": "^9.0.1",
    "core-js": "^3.18.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.3.0",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-html": "^6.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unicorn": "27.0.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.3.3",
    "friendly-errors-webpack-plugin": "npm:@soda/friendly-errors-webpack-plugin@^1.8.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^4.3.0",
    "image-minimizer-webpack-plugin": "^2.2.0",
    "lint-staged": "^10.5.1",
    "mini-css-extract-plugin": "^2.3.0",
    "npm-run-all": "^4.1.5",
    "ora": "^6.0.1",
    "postcss": "^8.3.8",
    "postcss-loader": "^6.1.1",
    "prettier": "^2.2.1",
    "resolve-url-loader": "^4.0.0",
    "sass": "^1.42.1",
    "sass-loader": "^12.1.0",
    "seng-generator": "^0.9.0",
    "sort-package-json": "^1.52.0",
    "style-loader": "^3.3.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.57.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.3.1"
  },
  "engines": {
    "node": ">=14",
    "npm": ">=7"
  },
  "publishConfig": {
    "access": "public"
  },
  "pota": {
    "excludedFiles": [
      "build-tools"
    ],
    "renames": {
      "gitignore": ".gitignore"
    },
    "package.json": {
      "name": "webpack-project",
      "scripts": {
        "check-types": "tsc --project tsconfig.json --noEmit --noUnusedLocals",
        "fix": "npm-run-all fix:*",
        "fix:eslint": "npm run lint:eslint -- --fix",
        "fix:prettier": "prettier \"**/*.{js,jsx,ts,tsx,scss}\" --write --loglevel warn",
        "lint": "npm-run-all lint:*",
        "lint:eslint": "eslint . --ext .ts,.tsx --cache --cache-location node_modules/.cache/.eslintcache"
      }
    }
  }
}
